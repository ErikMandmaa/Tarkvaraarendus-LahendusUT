def failist_sonastik(failinimi): # Reads a file and creates a dictionary mapping codes to country names
    sonastik = {} # Initialize an empty dictionary
    with open(failinimi, 'r') as fail: # Open the file for reading
        for rida in fail: # Iterate over each line in the file
            osad = rida.strip().split(' ', 1) # Split the line into code and name
            tahis = osad[0] # The code is the first part
            nimi = osad[1] # The name is the second part
            sonastik[tahis] = nimi # Add the code-name pair to the dictionary
    return sonastik # Return the completed dictionary


def tahised_nimedeks(tahised, sonastik): # Converts a list of codes to their corresponding country names using the dictionary
    nimed = [] # Initialize an empty list to store country names
    for tahis in tahised: # Iterate over each code in the input list
        if tahis in sonastik: # If the code exists in the dictionary
            nimed.append(sonastik[tahis]) # Append the corresponding country name to the list
        else: # If the code does not exist in the dictionary
            nimed.append(None) # Append None to indicate an unknown country
    return nimed # Return the list of country names


failinimi = input("Sisestage andmebaasi faili nimi: ") # Prompt the user for the database file name
tahised_sone = input("Piiri√ºletajad: ") # Prompt the user for the string of country codes

sonastik = failist_sonastik(failinimi) # Create the dictionary from the file
tahised = tahised_sone.split() # Split the input string into a list of codes
nimed = tahised_nimedeks(tahised, sonastik) # Convert the codes to country names

print() # Print a blank line for better output formatting
for nimi in nimed: # Iterate over each country name in the list
    if nimi is None: # If the country name is None (unknown code)
        print("Tundmatu maa") # Print "Unknown country"
    else: # If the country name is known
        print(nimi) # Print the country name